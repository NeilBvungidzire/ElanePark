{
  "expo": {
    "name": "elanePark",
    "slug": "elanePark",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to show nearby parking spots."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyD2Mjwic3Wz4Ji4jVYZ8Y7m3coAgfaTluw"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.elanePark",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD2Mjwic3Wz4Ji4jVYZ8Y7m3coAgfaTluw"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
